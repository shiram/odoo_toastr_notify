<?xml version="1.0" encoding="UTF-8"?>

<!--
	Import Custom front end Assets
-->
<odoo>

<template id="odoo_notify_assets_backend" name="/odoo_notify_assets_backend" inherit_id="web.assets_backend">
	<xpath expr="//link[last()]" position="after">
		<link rel="stylesheet" href="/odoo_notify/static/src/css/toastr.min.css"/>
		<link rel="stylesheet" href="/odoo_notify/static/src/css/bsod.css"/>
	</xpath>

	<xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/odoo_notify/static/src/js/toastr.min.js"></script>
			<script type="text/javascript" src="/odoo_notify/static/src/js/bsod.js"></script>
        	<script type="text/javascript" src="/odoo_notify/static/src/js/exception_handler.js"></script>
		<script type="text/javascript" src="/odoo_notify/static/src/js/toastr_exception_handler.js"></script>
        </xpath>
</template>

</odoo>